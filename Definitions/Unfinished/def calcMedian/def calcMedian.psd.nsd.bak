<root xmlns:nsd="https://structorizer.fisch.lu" version="3.29" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preForIn="foreach" stepFor="by" author="jeine" created="2019-01-14" changedby="jeine" changed="2019-01-14" origin="Structorizer 3.29" text="&#34;def calcMedian(discreteValues)&#34;" comment="" color="ffffff" type="program" style="nice">
	<children>
		<instruction text="&#34;lenValues = len(discreteValues)&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;sortedValues = sorted(discreteValues)&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;medianIndex = lenValues / 2&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
		<alternative text="&#34;if (lenValues % 2 == 0)&#34;" comment="" color="ffffff" disabled="0">
